<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <DocumentationFile>bin\Debug\MyApi.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EmailValidation" Version="1.2.0"/>
    <PackageReference Include="Handlebars.Net" Version="2.1.6"/>
    <PackageReference Include="JsonPath.Net" Version="1.1.2"/>
    <PackageReference Include="MailKit" Version="4.7.1.1"/>
    <PackageReference Include="Microsoft.Extensions.Http.Resilience" Version="8.7.0"/>
    <PackageReference Include="Quartz.AspNetCore" Version="3.11.0"/>
    <PackageReference Include="Quartz.Serialization.Json" Version="3.11.0"/>
    <PackageReference Include="Riok.Mapperly" Version="3.6.0"/>
    <PackageReference Include="Serilog.AspNetCore" Version="8.0.2"/>
    <PackageReference Include="Serilog.Enrichers.Environment" Version="3.0.1"/>
    <PackageReference Include="Serilog.Enrichers.Thread" Version="4.0.0" />
    <PackageReference Include="Serilog.Logfmt" Version="1.0.3"/>
    <PackageReference Include="Serilog.Sinks.Async" Version="2.0.0" />
    <PackageReference Include="Serilog.Sinks.Console" Version="6.0.0" />
    <PackageReference Include="Serilog.Sinks.File" Version="6.0.0"/>
    <PackageReference Include="ThisAssembly" Version="1.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="IdentityModel" Version="7.0.0"/>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.7"/>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.7"/>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
    <PackageReference Include="NetEscapades.EnumGenerators" Version="1.0.0-beta08" PrivateAssets="all" ExcludeAssets="runtime">
      <IncludeAssets>compile; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="SqlSugarCore" Version="5.1.4.166"/>
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="out\web.config"/>
    <_ContentIncludedByDefault Remove="out\appsettings.json"/>
  </ItemGroup>

</Project>
